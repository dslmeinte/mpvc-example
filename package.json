{
  "name": "mpvc-example",
  "version": "0.0.1",
  "description": "Example of the Model-Projection-View-Controller Pattern.",
  "author": "Meinte Boersma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://dslmeinte@github.com/dslmeinte/mpvc-example"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "mobx": "3.6.2",
    "mobx-react": "4.2.2",
    "react": "15.6.2",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/lodash": "4.14.144",
    "browserify": "16.5.0",
    "http-server": "0.11.1",
    "tsify": "3.0.1",
    "tslib": "1.10.0",
    "typescript": "3.6.4",
    "watchify": "3.11.1"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "http-server -p 9000 web",
    "build": "browserify src/view/main.tsx -p [tsify] -o web/bundle.js",
